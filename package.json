{
  "name": "apollo-link-enums",
  "version": "0.1.2",
  "description": "Custom apollo link to parse custom enums, for apollo rest links",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/inhibitor1217/apollo-link-enums",
  "author": "inhibitor <inhibitor@kaist.ac.kr>",
  "license": "MIT",
  "scripts": {
    "prepare": "install-peers",
    "build": "run-s clean && run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "pre-commit": "yarn typecheck && yarn lint && yarn test",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "npx eslint",
    "test": "npx jest --coverage -c jest.config.js",
    "clean": "trash build"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.24.8",
    "@graphql-tools/schema": "^7.1.5",
    "@types/jest": "^26.0.23",
    "@types/lodash-es": "^4.17.6",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "install-peers-cli": "^2.2.0",
    "jest": "^27.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "trash-cli": "^5.0.0",
    "ts-jest": "^27.0.1",
    "typescript": "^5.3.3",
    "zen-observable-ts": "^1.0.0"
  },
  "dependencies": {
    "@apollo/client": "^3.3.19",
    "lodash-es": "^4.17.21"
  },
  "peerDependencies": {
    "graphql": "^14.0.0 || ^15.0.0 || ^16.0.0"
  }
}
